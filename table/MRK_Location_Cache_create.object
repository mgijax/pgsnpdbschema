cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table snp.MRK_Location_Cache (
        _Cache_key                 int             not null,
 	_Marker_key                int             not null,
	_Marker_Type_key	   int		   not null,
        _Organism_key              int             not null,
        chromosome                 varchar(8)      not null,
	sequenceNum                int             not null,
	cytogeneticOffset          varchar(20)     null,
	cmOffset                     float           null,
        startCoordinate            float           null,
        endCoordinate              float           null,
        strand                     char(1)         null,
        mapUnits                   varchar(50)     null,
        provider                   varchar(255)    null,
        version                    varchar(255)    null,
        _CreatedBy_key             int             not null,
	_ModifiedBy_key            int             not null,
	creation_date              timestamp without time zone DEFAULT now()        not null,
	modification_date          timestamp without time zone DEFAULT now()        not null 
);
EOSQL
